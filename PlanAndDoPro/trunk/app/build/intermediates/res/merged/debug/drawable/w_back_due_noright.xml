<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="fill">
        <shape android:shape="rectangle">
            <gradient
                android:startColor="@color/track_due_low"
                android:endColor="@color/track_due"
                android:angle="90"></gradient>
            <stroke
                android:width="1dip"
                android:color="#0000" />
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item android:left="-30dp">
        <shape
            android:shape="ring"
            android:innerRadius="1dp"
            android:thickness="1dp">
            <solid android:color="@color/track_due_chain" />
        </shape>
    </item>
    <item android:left="30dp">
        <shape
            android:shape="ring"
            android:innerRadius="1dp"
            android:thickness="1dp">
            <solid android:color="@color/track_due_chain" />
        </shape>
    </item>
    <item
        android:left="-1dp"
        android:right="38dp"
        >
        <shape android:shape="line">
            <stroke
                android:width="1dp"
                android:color="@color/track_due_chain" />
        </shape>
    </item>

